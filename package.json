{
  "name": "wf",
  "version": "0.0.1",
  "description": "Workflow command line assistant",
  "main": "index.js",
  "srcPath": "src",
  "buildPath": "lib",
  "srcGlob": "./src/**/*.js",
  "scripts": {
    "start": "node index.js",
    "clean": "rm -rf $npm_package_buildPath || true && mkdir $npm_package_buildPath",
    "build": "npm run clean && ./node_modules/.bin/webpack/webpack --config webpack.config.js",
    "eslint": "standard --verbose | snazzy",
    "lint": "yarn eslint",
    "test": "yarn lint",
    "precommit": "git diff --name-only --cached --relative | grep '\\.jsx\\?$' | xargs standard --verbose | snazzy",
    "prepush": "yarn test"
  },
  "bin": {
    "wf": "./bin/wf"
  },
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Neaox/wf.git"
  },
  "author": "Neaox",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Neaox/wf/issues"
  },
  "homepage": "https://github.com/Neaox/wf#readme",
  "dependencies": {
    "config": "^1.28.1",
    "jira": "^0.9.2",
    "vorpal": "^1.12.0"
  },
  "devDependencies": {
    "husky": "^0.14.3",
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "webpack": "^3.10.0"
  }
}
